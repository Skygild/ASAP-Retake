generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  phone     String   @unique
  firstName String?
  lastName  String?
  companyId String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
